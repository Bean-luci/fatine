package _01_User;

public class App {

	public static void main(String[] args) {
		
		
			try {
				User user= new User("duck","riri",12);
				System.out.println(user);
				
				user.setAge(-15);
				System.out.println(user);
				user.setAge(13);
				
				
				
				
			} catch (Exception e) {
				
				System.out.println(e.getMessage());
				
			}
			
			finally {
				
				
				System.out.print(" Le bloc finally est execute quoiqu'il arrive.avec ou sans exception");
				System.out.print(" Il est souvent utilisé pour libérer des ressources.(flux, connexion de BDD..)");
			}
		
			
	}

}
