package _07_Exo;

public class _03_Exo {

	public static void main(String[] args) {
		

	}
	public class NombrePair {
		 
	    public static void pariter(){
	        for (int i=2;i<=100;i++){
	            if (i % 2 == 0){
	                System.out.println(i);
	            }
	        }   
	    }
	    public static void main(String[] args) {
	        NombrePair.pariter();
	    }
	}
	

}
