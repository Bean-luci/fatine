package _01_User;

public class App {

	public static void main(String[] args) {
		
		
			try {
				User user= new User("duck","riri",12);
				System.out.println(user);
				
				user.setAge(-15);
				System.out.println(user);
				user.setAge(13);
				
				
				
				
			} catch (Exception e) {
				
				System.out.println(e.getMessage());
				
			}
		
			
	}

}
